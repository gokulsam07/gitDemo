package automation.cucumber;

import io.cucumber.java.en.Given;
import io.cucumber.java.en.Then;
import io.cucumber.java.en.When;

public class RegisterTest {
	@Given("when user opens register page")
	public void when_user_opens_register_page() {
		System.out.println();
	}

	@When("user enter firstname")
	public void user_enter_firstname() {
		System.out.println();
	}

	@When("user enter lastname")
	public void user_enter_lastname() {
		System.out.println();
	}

	@When("user enter new mail")
	public void user_enter_new_mail() {
		System.out.println();
	}

	@When("user enter telephone")
	public void user_enter_telephone() {
		System.out.println();
	}

	@When("user enter password")
	public void user_enter_password() {
		System.out.println();
	}

	@When("user confirms password")
	public void user_confirms_password() {
		System.out.println();
	}

	@When("user selects policy")
	public void user_selects_policy() {
		System.out.println();
	}

	@When("user clicks create")
	public void user_clicks_create() {
		System.out.println();
	}

	@Then("account should be created")
	public void account_should_be_created() {
	}	 System.out.println();
}
