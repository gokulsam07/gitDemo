package pkg.parent;
import java.util.LinkedList;

public class LinkedListExample {

	public static void main(String[] args) {

		LinkedList<String> ll = new LinkedList<String>();
		
		ll.add("Tom");
		ll.add("Dick");
		ll.add("Harry");
		ll.add(2,"ModifyThruAdd");
		
		System.out.println(ll);
		
		//Add in Front
		
		ll.addFirst("Front");
		//ll.set(3, null);
		System.out.println(ll);
		
		//Add in back
	ll.addLast("Last");
	System.out.println(ll);
	
	Object[] a=ll.toArray();
		
		
	}

}
